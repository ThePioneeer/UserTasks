<Window x:Class="UserTasks.Page1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UserTasks"
        mc:Ignorable="d"
        Title="Menu" Height="115" Width="250">
    <Grid Margin="5,5,5,5">
        <Grid.RowDefinitions>
            <RowDefinition Height="35"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        
        <Button Grid.Row="0" Content="Kasutajate haldus"
                Margin="0,0,0,5"
                Name="ButtonKasutajad" Click="ButtonKasutajad_Click"
                IsEnabled="False"/>
        <Button Grid.Row="1" Content="Ülesanded" Name="ButtonYlesanded" Click="ButtonYlesanded_Click"/>
    </Grid>
</Window>
